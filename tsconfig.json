{
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "compilerOptions": {
    "target": "es2019",
    "jsx": "react-jsx",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./lib/",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
